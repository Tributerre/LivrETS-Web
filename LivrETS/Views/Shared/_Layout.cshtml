<!doctype html>
<html lang="">
<head>
    <meta charset="utf-8" />
    <meta name="author" content="Charles Levesque, Willy Kouagnia" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/icon?family=Material+Icons">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" 
          integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap-datetimepicker.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/dropdowns-enhancement.css")">
    <!--<link rel="stylesheet" href="@Url.Content("~/Content/Site.css")" />-->

    <!-- Customizable CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/template/main.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/template/green.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/template/bootstrap-select.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/template/owl.carousel.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/template/owl.transitions.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/template/animate.min.css")">

    <link href="@Url.Content("~/Content/template/green.css")" rel="alternate stylesheet" title="Green color">


    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Asap:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <!-- Icons/Glyphs -->
    <link rel="stylesheet" href="@Url.Content("~/Content/font-awesome/css/font-awesome.min.css")">
    <!--<link rel="stylesheet" href="@Url.Content("~/Content/template/elegant-fonts.css")">-->
    <!-- Favicon -->
    <link rel="shortcut icon" href="@Url.Content("~/Images/template/favicon.ico")">

    @RenderSection("styles", required: false)
</head>
<body>
    <div class="wrapper">
        @Html.Partial("_Header")

        <div class="wrapper">
            @RenderBody()
        </div>    

        @Html.Partial("_Footer")
    </div>

    <script src="https://code.jquery.com/jquery-2.2.3.min.js"
            integrity="sha256-a23g1Nt4dtEYOj7bR+vTu7+T8VP13humZFBJNIYoEJo="
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" 
            integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" 
            crossorigin="anonymous"></script>
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/moment-fr-ca.js"></script>
    <script src="~/Scripts/bootstrap-datetimepicker.min.js"></script>
    <script src="~/Scripts/dropdowns-enhancement.js"></script>
    <script src="~/Scripts/material.spinner.min.js"></script>
    <script src="~/Scripts/bootstrap-notify.min.js"></script>

    <script src="~/Scripts/template/bootstrap-select.min.js"></script>
    <script src="~/Scripts/template/bootstrap-hover-dropdown.min.js"></script>
    <script src="~/Scripts/template/bootstrap-slider.js"></script>
    <script src="~/Scripts/template/echo.min.js"></script>
    <script src="~/Scripts/template/jquery.easing-1.3.min.js"></script>
    <script src="~/Scripts/template/flatui-controls.js"></script>
    <script src="~/Scripts/template/owl.carousel.min.js"></script>
    <script src="~/Scripts/template/wow.min.js"></script>
    <script src="~/Scripts/template/scripts.js"></script>

    <script src="~/Scripts/site.js"></script>
    <script src="@Url.Content("~/Scripts/template/switchstylesheet/switchstylesheet.js")"></script>


    @RenderSection("scripts", required: false)
</body>
</html>
